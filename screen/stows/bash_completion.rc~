# bash_completion

screen 2
stuff "cd ~/stow/^M"
stuff "ls -al^M"
stuff "declare -a command=( \$( ls ~/dot/bash/bash_completion/ --hide=*~ ) )^M"
stuff "echo ${command[@]}^M"
stuff "for c in \${command[@]} ; do diff -u ~/dot/bash/bash_completion/\$c ~/stow/bash_completion/\$c ; done^M"
stuff "for c in \${command[@]} ; do diff -u ~/dot/bash/bash_completion/\$c /etc/bash_completion.d/\$c ; done^M"
stuff "# rm ~/stow/bash_completion/*^M"
stuff "# for c in \${command[@]} ; do ln ~/dot/bash/bash_completion/\$c ~/stow/bash_completion/\$c ; done^M"
stuff "# stow -n --verbose=2 -t /etc/bash_completion.d -d ~drbean/stow -R bash_completion^M"

