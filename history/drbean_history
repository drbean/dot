# for league in BMA0036 GL00018 GL00019 GL00020 ; do cp ~/admin/082/nuu/082${league}.txt ~/admin/082/nuu/${league}.txt; done
# for league in BMA0036 GL00018 GL00019 GL00020 ; do cp ~/admin/082/vance/082${league}.txt ~/admin/082/vance/${league}.txt; done
#for league in chat   ; do Moosh -n cohort-create -i $league -c 4 $league ; done
for league in chat   ; do Moosh -n cohort-create -i $league -c 4 $league ; done
for league in GL00018   ; do Moosh -n cohort-create -i 082$league -c 4 082$league ; done
for league in GL00020   ; do Moosh -n cohort-create -i 082$league -c 11 082$league ; done
